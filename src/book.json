{
    "gitbook": "3.2.3",
    "author": "junhuatechlog",
    "plugins": [
        "katex",
        "katex-plus",
        "alerts",
        "github",
        "github-buttons",
        "splitter",
        "-sharing", "sharing-plus",
        "copy-code-button",
        "anchor-navigation-ex",
        "sectionx",
        "click-reveal"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/junhuatechlog/Play-with-ML-in-Python"
        },
        "github-buttons": {
            "repo": "junhuatechlog/Play-with-ML-in-Python",
            "types": [
                "star",
                "watch",
                "fork"
            ],
            "size": "small"
        },
        "anchor-navigation-ex": {
            "isShowTocTitleIcon": true,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        }
    }
}
